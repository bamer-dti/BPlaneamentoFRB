<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="borderpane" maxHeight="-Infinity" maxWidth="-Infinity"
            prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="bamer.ControllerEcraTabelaSMS">
    <center>
        <ScrollPane>
            <BorderPane.margin>
                <Insets bottom="3.0" left="3.0" right="10.0" top="3.0"/>
            </BorderPane.margin>
        </ScrollPane>
    </center>
    <bottom>
        <HBox alignment="CENTER_RIGHT">
            <children>
                <Button fx:id="butfechar" mnemonicParsing="false" onAction="#fechar" prefWidth="80.0" text="Sair"/>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </HBox>
    </bottom>
</BorderPane>
